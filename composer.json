{
    "name": "larapackages/interceptor",
    "type": "library",
    "description": "Laravel Interceptor Pattern",
    "require": {
        "php": "^7.2",
        "illuminate/container": "^6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26",
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Larapackages\\Interceptor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larapackages\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapackages\\Interceptor\\Providers\\InterceptorServiceProvider"
            ]
        }
    }
}
